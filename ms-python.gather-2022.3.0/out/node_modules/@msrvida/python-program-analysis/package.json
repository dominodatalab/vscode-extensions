  "scripts": {
    "build": "npm run build:es5 && npm run build:umd",
    "build:umd": "webpack --env.TARGET=umd",
    "build:es5": "webpack --env.TARGET=es5",
    "build:parser": "node ./node_modules/jison/lib/cli.js --outfile src/python3.js src/python3.jison && cpx src/python3.js dist/es5",
    "format:all": "prettier --single-quote --trailing-comma=es5 --tab-width=2 --print-width=120 --write \"{src,style}/**/*.{css,js,ts}\"",
    "clean": "rimraf dist",
    "compile": "tsc -p ./",
    "compile:esm": "tsc -m es6 --outDir dist/es6 -p ./",
    "watch": "tsc -watch -p ./",
    "webpack": "webpack",
    "genspec": "node dist/es5/genspec/main.js",
    "test": "jest --watch",
    "pipelineTest": "jest"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
